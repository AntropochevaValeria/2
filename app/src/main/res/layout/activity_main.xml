<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/assurance1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15pt"
        android:hint="@string/message_res"
        android:inputType="text"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="27dp"
        tools:ignore="MissingConstraints"
        android:autofillHints="" />

    <EditText
        android:id="@+id/assurance2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15pt"
        android:hint="@string/key_res"
        android:inputType="text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="96dp"
        android:autofillHints="" />

    <Button
        android:id="@+id/nulls"
        android:onClick="eloquent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15pt"
        android:text="@string/encrypt_res"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="171dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/implausible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15pt"
        android:autofillHints=""
        android:hint="@string/message_res"
        android:inputType="text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="96dp" />



</LinearLayout>